<div class="page-header">
  <h3>Likes</h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">

      <dt>Photo</dt>

      <% @user.liked_photos.each do |like| %>
      <tr>
        <dd><%= like.caption %><p><img src="<%= like.image %>"></p></dd>
      </tr>
      <% end %>
    </dl>
    
  </div>
</div>
