<div class="page-header">
  <h3>User <%= @user.id %></h3>
</div>


<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">

<tr> Username: <%= @user.username %></tr>
<tr> <p>Email: <%=  @user.email %></p></tr>


      <% @user.photos.each do |photo| %>
      <tr>
        <td><%= photo.caption %><p><img src="<%= photo.image %>"></p></td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
